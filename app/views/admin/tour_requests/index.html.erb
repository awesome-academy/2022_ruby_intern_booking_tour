<div class="col-md-12">
  <div class="list_tours_request">
    <table id="mytable" class="table table-bordred table-striped">
      <thead>
        <th><input type="checkbox" id="checkall" /></th>
        <th><%= t ".name" %></th>
        <th><%= t ".email" %></th>
        <th><%= t ".price" %></th>
        <th><%= t ".quantity" %></th>
        <th><%= t ".total_price" %></th>
        <th><%= t ".status" %></th>
        <th><%= t ".action" %></th>
      </thead>
      <tbody id="list_tours_request">
        <%= render @tour_requests %>
      </tbody>
    </table>
    <div id="list_tours_request_pagy">
      <%== pagy_bootstrap_nav @pagy %>
    </div>
  </div>
</div>
